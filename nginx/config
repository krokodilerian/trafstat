ngx_feature=
ngx_feature_name=
ngx_feature_run=no
ngx_feature_incs=
ngx_feature_path=
ngx_feature_libs=
ngx_feature_test=
have=NGX_HTTP_HEADERS . auto/have
. auto/feature

if [ $ngx_found = yes ]; then
    ngx_addon_name=ngx_http_log_udp_module
    HTTP_MODULES="$HTTP_MODULES ngx_http_log_udp_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS vporn/ngx_http_log_udp_module.c"
else
    cat << END
$0: error: unable to configure the ngx_http_log_udp_module.
END
    exit 1
fi

